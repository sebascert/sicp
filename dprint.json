{
  "markdown": {
    "textWrap":"always"
  },
  "exec": {
    "cwd": "${configDir}",

    "lineWidth": 80,
    "indentWidth": 4,
    "useTabs": false,
    "cacheKey": "1",
    "timeout": 30,

    "commands": [{
      "command": "scheme-format {{file_path}}",
      "exts": ["scm"],
      "stdin": false,
    }]
  },
  "excludes": [],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.19.0.wasm",
    "https://plugins.dprint.dev/exec-0.5.1.json@492414e39dea4dccc07b4af796d2f4efdb89e84bae2bd4e1e924c0cc050855bf"
  ]
}
